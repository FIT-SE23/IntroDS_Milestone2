@article{Krahn:1925,
author="Krahn, E.",
title="{\"Uber eine von Rayleigh formulierte Minimaleigenschaft des Kreises.}",
language="German",
journal="Math. Ann. ",
volume="94",
pages="97--100",
year="1925",
doi={10.1007/BF01208645},
abstract="{Verf. beweist, da{\ss} unter allen Gebieten gleichen Fl\"acheninhalts
    der Kreis f\"ur die Bandwertaufgabe $\varDelta u + \lambda u = 0$ (Randwerte
    Null) den kleinsten ersten Eigenwert liefert. Er gewinnt die L\"osung der
    Randwertaufgabe aus dem Dirichletschen Integral, formt es um, indem er den
    Wert der L\"osung und die Bogenl\"ange auf der Niveaulinie als neue
    Koordinaten einf\"uhrt. Damit ist das Problem auf die isoperimetrische
    Eigenschaft des Kreises zur\"uckgef\"uhrt. (IV 15.)}",
reviewer="{Freudenthal, H.; Dr. (Amsterdam)}",
}


@article{Faber:1923,
