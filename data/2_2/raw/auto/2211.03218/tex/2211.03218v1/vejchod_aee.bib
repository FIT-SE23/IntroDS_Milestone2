@article {DMP1Dweak:2007,
    AUTHOR = {{\v{S}}ol{\'{\i}}n, Pavel and Vejchodsk{\'y}, Tom{\'a}{\v{s}}},
     TITLE = {A weak discrete maximum principle for {$hp$}-{FEM}},
