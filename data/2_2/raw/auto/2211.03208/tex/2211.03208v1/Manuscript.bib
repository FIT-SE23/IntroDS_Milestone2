doi = {10.1038/s41586-021-03819-2},
file = {:home/jadzia/Dropbox/Profession/Sciences/Theory/General/Literature/Mendeley/Jumper et al. - 2021 - Highly accurate protein structure prediction with AlphaFold(2).pdf:pdf},
issn = {0028-0836},
journal = {Nature},
