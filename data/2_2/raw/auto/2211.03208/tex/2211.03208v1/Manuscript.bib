file = {:home/jadzia/Dropbox/Profession/Sciences/Theory/General/Literature/Mendeley/Bender et al. - 2021 - A practical guide to large-scale docking.pdf:pdf},
issn = {17502799},
journal = {Nature Protocols},
keywords = {VS,ultra-large},
mendeley-groups = {CADD/Theory/Stage/Hit Identification/Design},
mendeley-tags = {VS,ultra-large},
publisher = {Springer US},
title = {{A practical guide to large-scale docking}},
