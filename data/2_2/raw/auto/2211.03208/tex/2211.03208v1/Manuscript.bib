journal = {Biophysical Reviews},
keywords = {Docking accuracy,Flexible docking,Rigid body docking,review},
mendeley-groups = {CADD/SW & DB/SW,CADD/Theory/Methods/Docking and Scoring},
mendeley-tags = {review},
