@article{somuHybridDeepTransfer2021,
  title = {A Hybrid Deep Transfer Learning Strategy for Thermal Comfort Prediction in Buildings},
  author = {Somu, Nivethitha and Sriram, Anirudh and Kowli, Anupama and Ramamritham, Krithi},
  year = {2021},
