  langid = {english},
  keywords = {buildings,predicted mean vote,predicted percentage of dissatisfied,productivity,thermal comfort},
  file = {C\:\\Users\\worku\\Zotero\\storage\\6XH5UX7Y\\Bueno et al. - 2021 - Evaluating the Connection between Thermal Comfort .pdf}
}




