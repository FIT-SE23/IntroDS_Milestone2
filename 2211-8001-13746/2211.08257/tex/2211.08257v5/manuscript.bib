  langid = {english},
  keywords = {adaptive thermal comfort,infrared thermography,intelligent workplace,random forest,support vector machine},
  file = {C\:\\Users\\worku\\Zotero\\storage\\78KXLNEF\\Lu et al. - 2019 - Thermal Comfort-Based Personalized Models with Non.pdf;C\:\\Users\\worku\\Zotero\\storage\\LK9FT86H\\1768.html}
