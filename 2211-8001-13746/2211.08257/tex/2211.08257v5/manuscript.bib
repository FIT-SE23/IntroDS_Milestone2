@article{jinDeepLearningBasedSchemeDetecting2020,
  title = {A {{Deep-Learning-Based Scheme}} for {{Detecting Driver Cell-Phone Use}}},
  author = {Jin, Chongchong and Zhu, Zhongjie and Bai, Yongqiang and Jiang, Gangyi and He, Anqing},
  year = {2020},
