}

@article{hulaSpinwaveFrequencyCombs2022,
  title = {Spin-Wave Frequency Combs},
  author = {Hula, T. and Schultheiss, K. and Gon{\c c}alves, F. J. T. and K{\"o}rber, L. and Bejarano, M. and Copus, M. and Flacke, L. and Liensberger, L. and Buzdakov, A. and K{\'a}kay, A. and Weiler, M. and Camley, R. and Fassbender, J. and Schultheiss, H.},
