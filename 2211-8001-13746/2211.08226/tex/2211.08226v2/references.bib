@article{hulaNonlinearLossesMagnon2020,
  title = {Nonlinear Losses in Magnon Transport Due to Four-Magnon Scattering},
  author = {Hula, Tobias and Schultheiss, Katrin and Buzdakov, Aleksandr and K{\"o}rber, Lukas and Bejarano, Mauricio and Flacke, Luis and Liensberger, Lukas and Weiler, Mathias and Shaw, Justin M. and Nembach, Hans T. and Fassbender, J{\"u}rgen and Schultheiss, Helmut},
