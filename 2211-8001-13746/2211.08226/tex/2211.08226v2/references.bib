@article{komineasTopologyDynamicsFerromagnetic1996,
  title = {Topology and Dynamics in Ferromagnetic Media},
  author = {Komineas, S and Papanicolaou, N},
  year = {1996},
  month = dec,
  journal = {Physica D: Nonlinear Phenomena},
  volume = {99},
  number = {1},
  pages = {81--107},
  abstract = {A direct link between the topological complexity of ferromagnetic media and their dynamics has recently been established through the construction of unambiguous conservation laws as moments of a topological vorticity. In the present paper we carry out this program under completely realistic conditions, with due account of the long-range magnetostatic field and related boundary effects. In particular, we derive unambiguous expressions for the linear and angular momentum in a ferromagnetic film which are then used to study the dynamics of magnetic bubbles under the influence of an applied magnetic-field gradient. The semi-empirical golden rule of bubble dynamics is verified in its gross features but not in its finer details. A byproduct of our analysis is a set of virial theorems generalizing Derrick's scaling relation as well as a detailed recalculation of the fundamental magnetic bubble.}
}

@article{komineasTransmutationMomentumPosition2008,
  title = {Transmutation of Momentum into Position in Magnetic Vortices},
  author = {Komineas, Stavros and Papanicolaou, Nikos},
  year = {2008},
  journal = {New Journal of Physics},
  volume = {10},
  number = {4},
  pages = {043021 (17pp)},
  abstract = {We show that transmutation of linear momentum into position may occur in a system of three magnetic vortices thanks to a direct link between topology and dynamics in a ferromagnet. This happens via an exchange between the linear momentum of a vortex-antivortex (VA) pair and the position of a single vortex during a semi-elastic scattering process. Vortex polarity switching occurs in the case of inelastic collisions.}
}

@article{komineasVirialTheoremVortices2006,
  title = {A Virial Theorem for Vortices in Ferromagnetic Elements},
  author = {Komineas, S},
  year = {2006},
  journal = {J. Phys. A.},
  volume = {39},
  pages = {5669--5679},
  doi = {10.1088/0305-4470/39/20/003},
  abstract = {We derive a virial theorem for a disc-shaped ferromagnetic particle with an axially symmetric magnetic configuration. This is a generalization of Derrick's scaling theorem which is now valid in the presence of surfaces. The relation gives simple results when applied to elementary magnetic states such as a single domain in an infinitely elongated cylinder. We further calculate the vortex state and verify numerically that it satisfies the virial relation. The vortex profile has a simple form in the limit of a very thin particle where also the virial relation simplifies and effectively gives the vortex core radius. Away from the very thin limit, the vortex configuration becomes more complicated with a varying vortex core radius along the thickness of the particle.}
}

@article{komineasVortexDynamicsTwodimensional1998,
  title = {Vortex Dynamics in Two-Dimensional Antiferromagnets},
  author = {Komineas, S and Papanicolaou, N},
  year = {1998},
  journal = {Nonlinearity},
  volume = {11},
  number = {2},
  pages = {265--290},
  abstract = {Vortices or merons are the relevant topological solitons in a two-dimensional isotropic Heisenberg antiferromagnet immersed in a uniform magnetic field. The dynamics of such solitons is studied numerically within the discrete spin model as well as analytically within a continuum approximation based on a suitable extension of the nonlinear IMG model. Vortex dynamics is affected rather profoundly by the applied field and acquires the characteristic features of the Hall effect of electrodynamics or the Magnus effect of fluid dynamics. In particular, a single vortex is always spontaneously pinned, two like vortices form a rotating bound state, and a vortex-antivortex pair undergoes Kelvin motion.}
}

