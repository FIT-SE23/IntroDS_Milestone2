@article{brandtEffectiveExchangeInteraction2021,
  title = {Effective Exchange Interaction for Terahertz Spin Waves in Iron Layers},
  author = {Brandt, Liane and Ritzmann, Ulrike and Liebing, Niklas and Ribow, Mirko and Razdolski, Ilya and Brouwer, Piet and Melnikov, Alexey and Woltersdorf, Georg},
