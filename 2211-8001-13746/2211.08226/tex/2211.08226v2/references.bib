@article{ilgazDomainWallDepinningAssisted2010,
  title = {Domain-{{Wall Depinning Assisted}} by {{Pure Spin Currents}}},
  author = {Ilgaz, D and Nievendick, J and Heyne, L and Backes, D and Rhensius, J and Moore, T.\${\textbackslash}sim\$A. and Ni{\~n}o, M.\${\textbackslash}sim\${\'A}. and Locatelli, A and Mente{\c s}, T.\${\textbackslash}sim\$O. and V.\${\textbackslash}sim\$Schmidsfeld, A and V.\${\textbackslash}sim\$Bieren, A and Krzyk, S and Heyderman, L.\${\textbackslash}sim\$J. and Kl{\"a}ui, M},
  year = {2010},
  month = aug,
  journal = {Physical Review Letters},
  volume = {105},
  number = {7},
  pages = {76601},
  doi = {10.1103/PhysRevLett.105.076601},
  abstract = {We study the depinning of domain walls by pure diffusive spin currents in a nonlocal spin valve structure based on two ferromagnetic Permalloy elements with copper as the nonmagnetic spin conduit. The injected spin current is absorbed by the second Permalloy structure with a domain wall, and from the dependence of the wall depinning field on the spin current density we find an efficiency of 6?10-14T/(A/m2), which is more than an order of magnitude larger than for conventional current induced domain-wall motion. Theoretically we find that this high efficiency arises from the surface torques exerted by the absorbed spin current that lead to efficient depinning.},
  keywords = {Domain walls and domain structure,Spin polarized transport in metals},
  annotation = {\_eprint: 1008.2773}
}

