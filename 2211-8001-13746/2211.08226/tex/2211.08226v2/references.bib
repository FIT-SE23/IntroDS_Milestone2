@article{gladiiSpinwavePropagationSpinpolarized2017,
  title = {Spin-Wave Propagation and Spin-Polarized Electron Transport in Single-Crystal Iron Films},
  author = {Gladii, O. and Halley, D. and Henry, Y. and Bailleul, M.},
  year = {2017},
  month = nov,
  journal = {Physical Review B},
  volume = {96},
  number = {17},
  pages = {174420},
  publisher = {{American Physical Society}},
  issn = {24699969},
  doi = {10.1103/PhysRevB.96.174420},
  abstract = {The techniques of propagating spin-wave spectroscopy and current-induced spin-wave Doppler shift are applied to a 20-nm-thick Fe/MgO(001) film. The magnetic parameters extracted from the position of the spin-wave resonance peaks are very close to those tabulated for bulk iron. From the zero-current propagating wave forms, a group velocity of 4 km/s and an attenuation length of about 6 {$\mu$}m are extracted for 1.6-{$\mu$}m-wavelength spin wave at 18 GHz. From the measured current-induced spin-wave Doppler shift, we extract a surprisingly high degree of spin polarization of the current of 83\%, which constitutes the main finding of this work. This set of results makes single-crystalline iron a promising candidate for building devices utilizing high-frequency spin waves and spin-polarized currents.},
  annotation = {\_eprint: 1704.02217}
}

@article{gladilinStableAntiferromagneticVortex2012,
  title = {Stable `antiferromagnetic' Vortex Lattice Imprinted into a Type-{{II}} Superconductor},
  author = {Gladilin, V N and Tempere, J and Devreese, J T and Moshchalkov, V V},
  year = {2012},
  journal = {New Journal of Physics},
  volume = {14},
  number = {10},
  pages = {103021},
  abstract = {In type-II superconductors, where vortices and antivortices tend to annihilate, only a `ferromagnetic' vortex lattice, with the same orientation of vortex magnetic moments, is usually formed in a homogeneous external magnetic field. Using the time-dependent Ginzburg\textendash Landau formalism, we demonstrate that a checkerboard vortex\cyrchar\cyrv\cyrchar\CYRDJE ``antivortex lattice (`antiferromagnetic vortex lattice'), imprinted onto a superconducting film by a periodic array of underlying clockwise and counterclockwise microcoils generating spatially periodic positive and negative magnetic field pulses and then trapped by an array of artificial pinning centers, remains stable even after the imprinting magnetic field pulse is switched off.}
}

