@article{bazeiaSemicompactSkyrmionlikeStructures2017,
  title = {Semi-Compact Skyrmion-like Structures},
  author = {Bazeia, D and Rodrigues, E I B},
  year = {2017},
  month = jun,
  journal = {Journal of Magnetism and Magnetic Materials},
  volume = {432},
  pages = {112--118},
  publisher = {{Elsevier BV}},
  doi = {10.1016/j.jmmm.2017.01.067},
  abstract = {We study three distinct types of planar, spherically symmetric and localized structures, one of them having non-topological behavior and the two others being of topological nature. The non-topological structures have energy density localized in a compact region in the plane, but are unstable against spherically symmetric fluctuations. The topological structures are stable and behave as vortices and skyrmions at larger distances, but they engender interesting compact behavior as one approaches their inner cores. They are semi-compact skyrmion-like spin textures generated from models that allow to control the internal behavior of such topological structures.}
}

