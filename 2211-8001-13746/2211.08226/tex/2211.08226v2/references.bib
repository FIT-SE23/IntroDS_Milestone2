@article{muxworthyHightemperatureMagneticStability2003,
  title = {High-Temperature Magnetic Stability of Small Magnetite Particles},
  author = {Muxworthy, Adrian R and Dunlop, David J and Williams, Wyn},
  year = {2003},
  month = may,
  journal = {J. Geophys. Res.},
  volume = {108},
  number = {B5},
  pages = {2281----},
  publisher = {{AGU}},
  issn = {0148-0227},
  abstract = {The stability of magnetic domain structures of small grains of magnetite were examined between room temperature and the Curie temperature using a high-resolution three-dimensional micromagnetic algorithm. At all times the minimum resolution used was determined by calculating the exchange length. Using an unconstrained model, the single domain (SD) to multidomain (MD) threshold grain size d 0 was found to be nearly independent of temperature up to \&\#8764;450\textdegree C. Above this temperature, d 0 was observed to rise sharply. Energy barriers between metastable domain states trapped in local energy minimums (LEM) were determined using a constrained algorithm. Three types of domain structure were considered: SD, vortex, and double vortex (effectively three domain), in a range of grain sizes with side length between 30 and 300 nm. In addition, the effect of varying shape was also considered by examining asymmetric grains with aspect ratios up to 1.4. From the numerical solutions energy barriers between LEM states were determined. It was found that MD grains 300 nm in size display higher stability than smaller SD grains (\&\#8764;50 nm). Double vortex states were found to be less stable than single vortex states at nearly all temperatures. Blocking temperatures as function of grain size for both symmetric and asymmetric grains were determined and agree well with experimental results. Transdomain thermoremanence analysis indicated that there are a limited number of grain sizes and shapes which will nucleate domain wall-type structures during cooling. Such nucleation events would cause the total measured remanence to decrease with cooling in conflict with N\cyrchar\cyrishrt el's analytical theory for remanence cooling behavior but in agreement with experimental observations.},
  keywords = {1521 Geomagnetism and Paleomagnetism: Paleointensi,1533 Geomagnetism and Paleomagnetism: Remagnetiza,1540 Geomagnetism and Paleomagnetism: Rock and mi,1599 Geomagnetism and Paleomagnetism: General or}
}

@article{myersCurrentInducedSwitching1999,
  title = {Current\textendash Induced Switching of Domains in Magnetic Multilayer Devices},
  author = {Myers, E B and Ralph, D C and Katine, J A and Louie, R N and Buhrman, R A},
  year = {1999},
  journal = {Science},
  volume = {285},
  pages = {867--870},
  doi = {10.1126/science.285.5429.867},
  abstract = {Current-induced switching in the orientation of magnetic moments is observed in cobalt/copper/cobalt sandwich structures, for currents flowing perpendicularly through the layers. Magnetic domains in adjacent cobalt layers can be manipulated controllably between stable parallel and antiparallel configurations by applying current pulses of the appropriate sign. The observations are in accord with predictions that a spin-polarized current exerts a torque at the interface between a magnetic and nonmagnetic metal, due to local exchange interactions between conduction electrons and the magnetic moments.}
}

