@article{ivanovTwodimensionalSolitonDynamics1989,
  title = {Two-Dimensional Soliton Dynamics in Ferromagnets},
  author = {Ivanov, B A and Stephanovich, V A},
  year = {1989},
  month = oct,
  journal = {Physics Letters A},
  volume = {141},
  number = {1-2},
  pages = {89--94},
  abstract = {The dynamical properties of topological precessional solitons in two-dimensional ferromagnets with uniaxial anisotropy are considered. It turns out that free (without external forces acting) soliton motion corresponds to some rotary motion (analogous to Larmor precession of a charged particle in an external magnetic field). The effective mass of the soliton is found. It is shown that the denominator of the mass value contains the anisotropy constant, so in the isotropic ferromagnet the topological soliton motion is impossible.}
