@article{wysinSinglekinkDynamicsEasyplane1986,
  title = {Single-Kink Dynamics in an Easy-Plane Classical Antiferromagnetic Chain},
  author = {Wysin, G M and Bishop, A and Oitmaa, Jaan},
  year = {1986},
  month = jan,
  journal = {Journal of Physics C: Solid State Physics},
  volume = {19},
  number = {2},
  pages = {221--233},
  publisher = {{IOP Publishing}},
  doi = {10.1088/0022-3719/19/2/017},
  abstract = {The authors present an ansatz for kink excitations in an easy-plane classical antiferromagnetic chain with a magnetic field in the easy plane. The utility of the ansatz is that it presents the in-plane (XY) and out-of-plane (YZ) kinks as belonging to one continuously connected energy dispersion curve. Linear stability analysis applied to YZ kinks shows that there is a velocity-dependent critical field necessary for stability. The authors also present a set of results of a numerical integration of the equations of motion which verifies the YZ kink stability regimes, as well as showing that XY kinks are stable over a wide range of fields and velocities.}
}

