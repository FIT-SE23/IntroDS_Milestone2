@article{floratosLagrangianResolutionMomentum1992,
  title = {A Lagrangian Resolution of the Momentum Problem for Two Dimensional Ferromagnets},
  author = {Floratos, E G},
  year = {1992},
  month = apr,
  journal = {Physics Letters B},
  volume = {279},
  number = {1-2},
  pages = {117--123},
  abstract = {The Landau-Lifshitz model of ferromagnetism in 2+1 dimensions is plagued with ambiguities in the definition of the linear and angular momentum. We discuss these problems in the language of the CP1 non-relativistic model in 2+1 dimensions. The Noether procedure applied to the topologically non-trivial sector of this lagrangian model determines uniquely a well-defined linear and angular momentum. Surprisingly enough, translational invariance is broken by the topological charge. This phenomenon appears in 2+1 dimensions only for the non-relativistic version of the CP1 model.}
}

