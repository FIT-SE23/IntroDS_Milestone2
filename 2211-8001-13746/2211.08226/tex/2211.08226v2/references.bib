@article{zolotaryukResonantTunnellingShortrange2007,
  title = {Resonant Tunnelling through Short-Range Singular Potentials},
  author = {Zolotaryuk, A V and Christiansen, P L and Iermakova, S V},
  year = {2007},
  journal = {Journal of Physics A: Mathematical and Theoretical},
  volume = {40},
  number = {20},
  pages = {5443},
  abstract = {A three-parameter family of point interactions constructed from sequences of symmetric barrier\cyrchar\cyrv\cyrchar\CYRDJE ``well\cyrchar\cyrv\cyrchar\CYRDJE ``barrier and well\cyrchar\cyrv\cyrchar\CYRDJE ``barrier\cyrchar\cyrv\cyrchar\CYRDJE ``well rectangles is studied in the limit, when the rectangles are squeezed to zero width but the barrier height and the well depth become infinite (the zero-range limit). The limiting generalized potentials are referred to as the second derivative of Dirac's delta function \cyrchar\CYRV{$\pm$}\cyrchar\CYRO\guillemotright\cyrchar\CYRO\cyrchar\cyrgup ''( x ) with a renormalized coupling constant \cyrchar\CYRO\guillemotright{} {$>$} 0 or simply as \cyrchar\CYRV{$\pm$}\cyrchar\CYRO\cyrchar\cyrgup ''-like point interactions. As a result, a whole family of self-adjoint extensions of the one-dimensional Schr\cyrchar\CYRG\textbackslash Pdinger operator is shown to exist, which results in full and partial resonant tunnelling through this class of singular potentials. The resonant tunnelling occurs for countable sets of interaction strength values in the \cyrchar\CYRO\guillemotright -space which are the roots of several transcendental equations. The comparison with the previous results for \cyrchar\CYRO\cyrchar\cyrgup '-like point interactions is also discussed.}
}

@article{zolotaryukScatteringPropertiesPoint2006,
