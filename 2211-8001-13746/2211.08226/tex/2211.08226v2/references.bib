@article{leeseQLumpsTheirInteractions1991,
  title = {Q-{{Lumps}} and Their Interactions},
  author = {Leese, R A},
  year = {1991},
  month = dec,
  journal = {Nuclear Physics B},
  volume = {366},
  number = {2},
  pages = {283--311},
  abstract = {The nonlinear O(3) [sigma]-model in (2 + 1) dimensions, modified by the addition of a potential term, admits solutions of Q-ball type. Such configurations, given the name Q-lumps, carry an integer-valued topological charge in addition to the conserved Noether charge. For a special choice of potential, they may be constructed explicitly in terms of simple functions via a natural extension of the usual Bogomolny equations. The existence of a Bogomolny bound guarantees that Q-lumps minimize the energy for any given value of Q. Their stability is discussed in detail and their interactions are investigated for low impact velocities using an adiabatic approximation. Forces transverse to the direction of motion lead to many possibilities, including circular orbits. Even head-on collisions have exotic scattering angles.}
}

@article{leeSizeselectiveResonantExcitation2013,
  title = {Size-Selective Resonant Excitation of Soft Magnetic Nano-Spheres of Three-Dimensional Magnetic Vortex},
  author = {Lee, Jehyun and Yoo, Myoung-Woo and Han, Dong-Soo and Kim, Sang-Koog},
  year = {2013},
  month = nov,
  journal = {ArXiv e-prints},
  abstract = {Combined micromagnetic numerical and analytical calculations revealed strong resonant excitations of soft magnetic nano-spheres of a three-dimensional (3D) magnetic vortex, which excitations remarkably vary with the sphere diameter 2R. This function originates from the diameter-dependent frequency of precession of the 3D vortex core about the direction of an applied static field . The characteristic angular frequency was found to be expressed as \$\o mega\_3DV={$<$}m\_\textbackslash hatr{$>\_$}V\textbackslash gammaH\_DC\$, where \${$<$}m\_\textbackslash hatr{$>\_$}V\$ is the average magnetization component of the ground-state vortex structure in the core direction, and {$\gamma$} is the gyromagnetic ratio. Numerical calculations also revealed that \${$<$}m\_\textbackslash hatr{$>\_$}V\$ is a strong function of 2R for a given material, as \${$<$}m\_\textbackslash hatr{$>\_$}V=(73.6\textbackslash pm3.4)(l\_ex/2R)\^2.2\$ with the exchange length lex. Our results provide not only a new physical insight into 3D vortex dynamics but also their potential applications to size-selective resonant excitation and/or possible detection of magnetic nano-particles.}
}

