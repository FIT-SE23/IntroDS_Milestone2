  number = {0},
  pages = {981--983},
  issn = {0304-8853},
  abstract = {In 2D classical spin arrays subject to planar Heisenberg interaction Jxy, periodic metastable states of vortex\cyrchar\cyrv\cyrchar\CYRDJE ``antivortex pairs will be described. In the unit cell of 2n\cyrchar\CYRG\textendash 2n spins with the symmetry of planar group P4, two vortices and two antivortices are arranged. Their pair energy increases nearly logarithmically with pair distance n. In contrast, for isotropic interaction J the excitations are half-skyrmion-like and their pair energy is \cyrchar\cyrv\textperthousand\texteuro 4\cyrchar\CYRP\cyrchar\CYRDJE J(1\cyrchar\cyrv\cyrchar\CYRDJE ``1.02/n2), hence 4\cyrchar\CYRP\cyrchar\CYRDJE J in the continuous limit as for a single skyrmion. A collapse is prevented due to the increase of the mean energy per spin for decreasing distance n.},
  keywords = {2-magnetism,Classical spin arrays,Heisenberg interaction,Metastable ordered states,Skyrmion solid,Vortices}
}

@article{waldnerAreSkyrmions2D1992,
  title = {Are {{Skyrmions}} ({{2D}} Solitons) Observable in {{2D}} Antiferromagnets?},
  author = {Waldner, F},
  year = {1992},
  month = feb,
  journal = {J.\$\textbackslash sim\$Magn. Magn. Mater.},
  volume = {104--107},
  number = {Part 2},
  pages = {793--794},
  abstract = {For two-dimensional (2D)XY magnets the existence of localized excitations in addition to spin waves is well established for elevated temperatures. In contrast, for 2D Heisenberg antiferromagnets it is argued that localized excitations play no role for the spin correlation length [xi] although such excitations were described by Skyrme (Skyrmions) long ago. These arguments will be revisited. Further, the spatial measurements of the spin correlation length [xi] and the temporal observations as measured by the line broadening of electron spin resonance lines, both measured in the regime far above TN3D, will be compared with the model proposed by Belavin and Polyakov which considers thermal excitation of Skyrmions.}
}

