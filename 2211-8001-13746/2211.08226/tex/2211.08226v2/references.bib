@book{gaidideiCurvatureEffectsDynamics2013,
  title = {Curvature Effects in Dynamics of a Thin Magnetic Shell},
  author = {Gaididei, Yuri and Kravchuk, Volodymyr P and Sheka, Denis D},
  year = {2013},
  month = nov,
  journal = {ArXiv e-prints},
  abstract = {Equations of the magnetization dynamics are derived for an arbitrary curved 2D surface. The curvature results in two additional effective interactions originated from exchange: one is analogue of magnetic anisotropy, another one is analogue of Dzyaloshinskii interaction. Equilibrium state of the curvilinear magnetic nanoshel with easy-surface anisotropy can be understood as a competition between these effective interactions. As an illustration of our method we consider statics and dynamics of a truncated cone magnetic surface. To single out the curvature effects we study the continuous transition from planar ring to a cylinder surface by varying the inclination angle of the cone generatrix from 0 to \$\textbackslash pi/2\$.}
}

