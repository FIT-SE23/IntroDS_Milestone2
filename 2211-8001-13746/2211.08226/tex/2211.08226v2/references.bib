@article{swanAsymptoticPhaseshiftsBound1963,
  title = {Asymptotic Phase-Shifts and Bound States for Two-Body Central Interactions},
  author = {Swan, P},
  year = {1963},
  journal = {Nuclear Physics},
  volume = {46},
  pages = {669--694},
  abstract = {Levinson's theorem is proved for the elastic, two-body scattering of both neutral and of charged particles, for arbitrary angular momentum states. The necessary restriction on the interaction potential U(r) is r2U(r)=0 at R=0 and R=[infinity], with U(r{$>$}0) finite. The theorem also applies to the equivalent two-body potential between complex particles. Levinson's relation for the l-wave phase-shift [eta]l(k) gives [eta]l(0)-[eta]l([infinity])=[eta]l[pi], with [eta]l([infinity])=0, where [eta]l is the number of composite bound states. If there is an l=0 resonance at zero energy, this is modified to . Coulomb bound states do not obey this relation, as r2V(r)[not equal to]0 at r=[infinity]. The special case of the inverse square potential, in those cases where [eta]l(k) can be defined, gives the result [eta]l(k)=constant for all k. Attractive potential wells with repulsive singular cores, not obeying the restriction r2U(r)=0 at r=0, are shown to lead to [eta]l(0)=[eta]l[pi], [eta]l([ infinity])=-const.[kappa]p, 0[equal-or-less, slanted]p[equal-or-less, slanted]1, where p increases from 0 to 1 as the order of the singularity increases from 2 to [infinity]. The values of [eta]l(0) and [eta]l([infinity]) largely condition the shape of the [eta]l(k) function, which is otherwise arbitrary by integral multiples of [pi]. Examples of the variationof [eta]0(k) with k and potential well strength are given graphically for the square well. Their behaviour is related to the general properties [eta]l(k\textendash{$>$}0) [is proportional to] k2l+1 and [eta]l(k\textendash{$>$}[infinity]) [is proportional to] 1/k, and to the existence of shape resonance scattering masked by hard sphere (potential) scattering.}
}

