@article{yanChiralSymmetryBreaking2012,
  title = {Chiral Symmetry Breaking and Pair-Creation Mediated {{Walker}} Breakdown in Magnetic Nanotubes},
  author = {Yan, Ming and Andreas, Christian and K{\'a}kay, Attila and {Garc{\'i}a-S{\'a}nchez}, Felipe and Hertel, Riccardo},
  year = {2012},
  month = jun,
  journal = {Applied Physics Letters},
  volume = {100},
  number = {25},
  pages = {252401},
  publisher = {{American Institute of PhysicsAIP}},
  issn = {00036951},
  doi = {10.1063/1.4727909},
  abstract = {Field-driven domain wall (DW) propagation in ferromagnetic nanotubes displays unusual effects, as revealed by a micromagnetic study. The left-right symmetry of the DW dynamics is broken, yielding markedly different DW mobilities for opposite propagation directions. This asymmetry arises from the tubular geometry and its impact on the DW structure. Also, the Walker breakdown field and velocity are found to be asymmetric for opposite directions. In certain cases, the breakdown can even be suppressed in one or both directions. Topological constraint requires a vortex-antivortex pair mediated breakdown, contrary to the single (anti)vortex in flat strips. This results in a higher breakdown velocity. \textcopyright{} 2012 American Institute of Physics.},
  keywords = {chiral symmetries,ferromagnetic materials,magnet,magnetic domain walls,micromagnetics,nanomagnetics,nanotubes,vortices},
  file = {/Users/lukas/Zotero/storage/7S7C5I7V/Yan et al. - 2012 - Chiral symmetry breaking and pair-creation mediate.pdf;/Users/lukas/Zotero/storage/NGLYKHCU/Yan et al. - 2012 - Chiral symmetry breaking and pair-creation mediate.pdf;/Users/lukas/Zotero/storage/S7EMGE8C/Yan et al. - 2012 - Chiral symmetry breaking and pair-creation mediated Walker breakdown in magnetic nanotubes.pdf}
}

