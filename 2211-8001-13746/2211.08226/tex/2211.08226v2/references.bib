@book{ivanovExcitationSpinDynamics2007a,
  title = {Excitation of Spin Dynamics by Spin-Polarized Current in Vortex State Disks},
  author = {Ivanov, B.\${\textbackslash}sim\$A. and Zaspel, C.\${\textbackslash}sim\$E.},
  year = {2007},
  abstract = {A spin-polarized current with the polarization perpendicular to the plane of a vortex-state disk results in renormalization of the effective damping for a given magnetization mode, and the effective damping becomes zero if the current exceeds a threshold value. The lowest threshold current corresponds to the lowest frequency vortex gyroscopic mode. For larger values of the current the dynamic magnetization state is characterized by precession of the vortex around the dot center with non-small amplitude and higher frequency.}
}

@article{ivanovFineStructureSpectra2007,
  title = {Fine Structure of the Spectra of Magnetic Particles in the Vortex State and Their Ordered Arrays},
  author = {Ivanov, B and Bondarenko, P and Galkin, A and Zaspel, C and Sheka, D},
  year = {2007},
  month = nov,
  journal = {Bulletin of the Russian Academy of Sciences: Physics},
  volume = {71},
  number = {11},
  pages = {1494--1496},
  abstract = {A low-frequency (the frequency in the sub-GHz range) mode exist for submicron magnetic particles in the vortex state. This mode corresponds to oscillations of the vortex center. The other modes form doublets with higher (several gigahertz) frequency and small (about \cyrchar\CYRP\textperthousand VPM) splitting. Collective oscillations exist for lattices of such particles. The dependence of the frequency of these modes on the quasi-momentum can be nonanalytic.}
}

