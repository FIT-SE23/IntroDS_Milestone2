@article{boucherSolitonMagneticResonance1987,
  title = {Soliton {{Magnetic Resonance}} ({{SMR}}) in the {{Magnetic Chains}} of {{CsCoCl}}{$_{3}$}},
  author = {Boucher, J P and Rius, G and Henry, Y and Boucher, J P and Rius, G and Henry, Y},
  year = {1987},
  journal = {Europhys. Lett.},
  volume = {4},
  number = {9},
  pages = {1073--1078},
  abstract = {A soliton magnetic resonance (SMR) experiment has been performed on the antiferromagnetic Ising quantum spin chains of CsCoCl3, at the frequency of 35 GHz, with a magnetic field parallel (H[?]) or perpendicular (H[?]) to the chains. For the first time SMR signals have been identified in the 1D paramagnetic phase (T\$\textbackslash sim\$\${$>\$\$\backslash$}sim\$TN1\$\textbackslash sim\$[?]\$\textbackslash sim\$21\$\textbackslash sim\$K). They reveal a strong soliton damping and an appreciable resonant frequency renormalization, both associated with the soliton dynamics. For H[?], the SMR signals provide an evidence for the dyon precession described recently for magnetic chains. These SMR signals have been followed below TN1, confirming the persistence of solitons in the ordered phases of this compound.}
}

@book{boulleCurrentInducedDomain2013,
  title = {Current Induced Domain Wall Dynamics in the Presence of Spin Orbit Torques},
  author = {Boulle, O and {Buda-Prejbeanu}, L D and Ju{\'e}, E and Miron, I M and Gaudin, G},
  year = {2013},
  month = oct,
  abstract = {Current induced domain wall (DW) motion in perpendicularly magnetized nanostripes in the presence of spin orbit torques is studied. We show using micromagnetic simulations that the direction of the current induced DW motion and the associated DW velocity depend on the relative values of the field like torque (FLT) and the Slonczewski like torques (SLT). The results are well explained by a collective coordinate model which is used to draw a phase diagram of the DW dynamics as a function of the FLT and the SLT. We show that a large increase in the DW velocity can be reached by a proper tuning of both torques.}
}

