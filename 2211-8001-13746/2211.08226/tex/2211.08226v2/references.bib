@article{ishibashiSwitchableGiantNonreciprocal2020,
  title = {Switchable Giant Nonreciprocal Frequency Shift of Propagating Spin Waves in Synthetic Antiferromagnets},
  author = {Ishibashi, Mio and Shiota, Yoichi and Li, Tian and Funada, Shinsaku and Moriyama, Takahiro and Ono, Teruo},
  year = {2020},
  month = apr,
  journal = {Science Advances},
  volume = {6},
  number = {17},
  pages = {eaaz6931},
  issn = {2375-2548},
  doi = {10.1126/sciadv.aaz6931},
  abstract = {We report the giant nonreciprocity and the electrical switching of propagating spin waves in synthetic antiferromagnets.           ,              The nonreciprocity of propagating spin waves, i.e., the difference in amplitude and/or frequency depending on the propagation direction, is essential for the realization of spin wave\textendash based logic circuits. However, the nonreciprocal frequency shifts demonstrated so far are not large enough for applications because they originate from interfacial effects. In addition, switching of the spin wave nonreciprocity in the electrical way remains a challenging issue. Here, we show a switchable giant nonreciprocal frequency shift of propagating spin waves in interlayer exchange\textendash coupled synthetic antiferromagnets. The observed frequency shift is attributed to large asymmetric spin wave dispersion caused by a mutual dipolar interaction between two magnetic layers. Furthermore, we find that the sign of the frequency shift depends on relative configuration of two magnetizations, based on which we demonstrate an electrical switching of the nonreciprocity. Our findings provide a route for switchable and highly nonreciprocal spin wave\textendash based applications.},
  langid = {english},
  file = {/Users/lukas/Zotero/storage/R9Q72X9M/Ishibashi et al. - 2020 - Switchable giant nonreciprocal frequency shift of .pdf}
}

