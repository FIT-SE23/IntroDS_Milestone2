@article{vojkovicVortexantivortexPairsInduced,
  title = {Vortex-Antivortex Pairs Induced by Curvature},
  author = {Vojkovic, Smiljan and {Carvalho-Santos}, Vagson L and Fonseca, Jakson M and Nunez, Alvaro S},
  journal = {ArXiv e-prints},
  abstract = {We show that the curvature of nanomagnets can be used to induce chiral textures in the magnetization field. Among the phenomena related to the interplay between geometry and magnetic behavior at nanomagnets, an effective curvature-induced Dzyaloshinskii-Moriya interaction has been recently predicted. In this work, it is shown that a magnetization configuration consisting of two structures with opposite winding numbers (vortex and antivortex) appear as remanent states. It is shown that these topological chiral configurations are a result of the Dzyaloshinkii-Moriya interaction induced by curvature. In this way, the obtained results present a new form to produce stable vortices and antivortices by using nanomagnets with variable curvature.},
  keywords = {cond-mat.mes-hall}
}

