  author = {Brandt, L. and Verba, R. and Liebing, N. and Ribow, M. and Razdolski, I. and Tyberkevych, V. and Slavin, A. and Woltersdorf, G. and Melnikov, A.},
  year = {2021},
  month = oct,
  journal = {arXiv:2107.12812 [cond-mat]},
