@article{bjorkAnalysisMagneticField2010,
  title = {Analysis of the Magnetic Field, Force, and Torque for Two-Dimensional {{Halbach}} Cylinders},
  author = {Bj{\o}rk, R. and Bahl, C. R. H. and Smith, A. and Pryds, N.},
  year = {2010},
  month = jan,
  journal = {Journal of Magnetism and Magnetic Materials},
  volume = {322},
  number = {1},
  eprint = {1409.1712},
  eprinttype = {arxiv},
  pages = {133--141},
  issn = {03048853},
  doi = {10.1016/j.jmmm.2009.08.044},
  abstract = {The Halbach cylinder is a construction of permanent magnets used in applications such as nuclear magnetic resonance apparatus, accelerator magnets and magnetic cooling devices. In this paper the analytical expression for the magnetic vector potential, magnetic flux density and magnetic field for a two dimensional Halbach cylinder are derived. The remanent flux density of a Halbach magnet is characterized by the integer p. For a number of applications the force and torque between two concentric Halbach cylinders are important. These quantities are calculated and the force is shown to be zero except for the case where p for the inner magnet is one minus p for the outer magnet. Also the force is shown never to be balancing. The torque is shown to be zero unless the inner magnet p is equal to minus the outer magnet p. Thus there can never be a force and a torque in the same system.},
  archiveprefix = {arXiv},
  langid = {english},
  keywords = {Condensed Matter - Other Condensed Matter,Halbach cylinder,Magnetic field,Magnetic force,Magnetic torque,Permanent magnet flux source,Physics - Classical Physics},
  file = {/Users/lukas/Zotero/storage/PMGTP8ZN/Bjørk et al. - 2010 - Analysis of the magnetic field, force, and torque .pdf;/Users/lukas/Zotero/storage/TH7QM77P/1409.1712.pdf}
}

