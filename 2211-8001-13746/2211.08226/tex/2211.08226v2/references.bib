@article{tyberkevychVectorHamiltonianFormalism2020,
  title = {Vector {{Hamiltonian Formalism}} for {{Nonlinear Magnetization Dynamics}}},
  author = {Tyberkevych, Vasyl and Slavin, Andrei and Artemchuk, Petro and Rowlands, Graham},
  year = {2020},
  month = nov,
  journal = {arXiv:2011.13562 [cond-mat]},
  eprint = {2011.13562},
  eprinttype = {arxiv},
  primaryclass = {cond-mat},
  abstract = {Vector Hamiltonian formalism (VHF) for the description of a weakly nonlinear magnetization dynamics has been developed. Transformation from the traditional Landau-Lifshitz equation, describing dynamics of a magnetization vector \$\textbackslash vec\{m\}(\textbackslash vec\{r\}, t)\$ on a sphere, to a vector Hamiltonian equation, describing dynamics of a \textbackslash emph\{spin excitation vector\} \$\textbackslash vec\{s\}(\textbackslash vec\{r\}, t)\$ on a plane, is done using the azimuthal Lambert transformation that preserves both the phase-space area and vector structure of dynamical equations, and guarantees that the plane containing vector \$\textbackslash vec\{s\}(\textbackslash vec\{r\}, t)\$ is at each value of the coordinate \$\textbackslash vec\{r\}\$ perpendicular to the a stationary vector \$\textbackslash vec\{m\}\_0(\textbackslash vec\{r\})\$ describing the magnetization ground state of the system. By expanding vector \$\textbackslash vec\{s\}(\textbackslash vec\{r\}, t)\$ in a complete set of linear magnetic vector eigemodes \$\textbackslash vec\{s\}\_\textbackslash nu(\textbackslash vec\{r\})\$ of the studied system, and using a weakly nonlinear approximation \$|\textbackslash vec\{s\}(\textbackslash vec\{r\}, t)| \textbackslash ll 1\$, it is possible to express the Hamiltonian function of the system in the form of integrals over the vector eigenmode profiles \$\textbackslash vec\{s\}\_\textbackslash nu(\textbackslash vec\{r\})\$, and calculate all the coefficients of this Hamiltonian. The developed approach allows one to describe weakly nonlinear dynamics in micro- and nano-scale magnetic systems with complicated geometries and spatially non-uniform ground states by numerically calculating linear spectrum and eigenmode profiles, and semi-analytically evaluating amplitudes of multi-mode nonlinear interactions. Examples of applications of the developed formalism to the magnetic systems having spatially nonuniform ground state of magnetization are presented.},
  archiveprefix = {arXiv},
  langid = {english},
  keywords = {Condensed Matter - Materials Science},
  file = {/Users/lukas/Zotero/storage/28I5LVGT/Tyberkevych et al. - 2020 - Vector Hamiltonian Formalism for Nonlinear Magneti.pdf;/Users/lukas/Zotero/storage/HPGDQ66N/2011.13562.pdf}
}

