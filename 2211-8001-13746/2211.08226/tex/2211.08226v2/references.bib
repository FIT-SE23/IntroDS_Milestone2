@article{mcmichaelHeadHeadDomain1997,
  title = {Head to Head Domain Wall Structures in Thin Magnetic Strips},
  author = {McMichael, R D and Donahue, M J},
  year = {1997},
  journal = {IEEE Transactions on Magnetics},
  volume = {33},
  number = {5},
  pages = {4167--4169},
  doi = {10.1109/20.619698},
  abstract = {We present calculations of head to head domain wall structures in magnetic strips of Ni80Fe20 with widths, w, ranging from 75 nm to 500 nm and thicknesses, t, from 1 nm to 64 nm. Neglecting magnetocrystalline and magnetostrictive anisotropy energies, minimization of exchange and magnetostatic energy leads to one of two types of domain wall structures: `transverse' walls with magnetization at the center of the wall directed transverse to the strip axis and `vortex' walls where the magnetization forms a vortex at the center of the wall. Calculation of the domain wall energies leads to a proposed phase diagram for head to head domain walls where transverse walls have lower energy when dimensions are less than tcritwcrit 130 A/mu0Ms2.}
}

