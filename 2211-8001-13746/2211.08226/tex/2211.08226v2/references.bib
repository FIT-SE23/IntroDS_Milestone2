@article{yershovCurvatureInducedDomain2015,
  title = {Curvature Induced Domain Wall Pinning},
  author = {Yershov, Kostiantyn V and Kravchuk, Volodymyr P and Sheka, Denis D and Gaididei, Yuri},
  year = {2015},
  month = may,
  journal = {ArXiv e-prints},
  abstract = {It is shown that a local bend of a nanowire is a source of pinning potential for a transversal head-to-head (tail-to-tail) domain wall. Eigenfrequency of the domain wall free oscillations at the pinning potential is determined as a function of the curvature and domain wall width. The pinning potential originates from the effective curvature induced Dzyaloshinsky-like term in the exchange energy. The theoretical results are verified by means of micromagnetic simulations for the case of parabolic shape of the wire bend.}
}

@book{yershovCurvatureTorsionEffects2015,
  title = {Curvature and Torsion Effects in the Spin-Current Driven Domain Wall Motion},
  author = {Yershov, Kostiantyn V and Kravchuk, Volodymyr P and Sheka, Denis D and Gaididei, Yuri},
  year = {2015},
  month = nov,
  journal = {ArXiv e-prints},
  abstract = {The domain wall motion along a helix-shaped nanowire is studied for the case of spin-current driving via Bazaliy-Zhang-Li mechanism. The analysis is based on collective variable approach. Two new effects are ascertained: (i) the curvature results in appearance of the Walker limit for a uniaxial wire, (ii) the torsion results in effective shift of the nonadiabatic spin torque parameter {$\beta$}. The latter effect changes considerably the domain wall velocity and can result in negative domain wall mobility. This effect can be also used for an experimental determination of the nonadiabatic parameter {$\beta$} and damping coefficient {$\alpha$}.}
}

