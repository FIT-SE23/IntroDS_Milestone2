@article{traaSymmetryBreakingAndersonHubbard1992,
  title = {Symmetry Breaking in the {{Anderson-Hubbard}} Model},
  author = {Traa, M R M J and Caspers, W J},
  year = {1992},
  month = apr,
  journal = {Physica A: Statistical and Theoretical Physics},
  volume = {183},
  number = {1-2},
  pages = {175--186},
  publisher = {{Elsevier BV}},
  doi = {10.1016/0378-4371(92)90184-R},
  abstract = {The Anderson-Hubbard (A-H) model with one or two holes and with periodic boundary conditions on a 4M x 4N square lattice is considered. On grounds of an intuitive generalization of Marshall's theorem we split the A-H Hamiltonian (HA-H) into a zeroth order term (H0) and a perturbation term (H'). With H0 we construct unfrustrated states: the zeroth order approximation of the degenerate ground state (GS). The one-hole system has a four-fold symmetry broken H0-GS with K = ([pi]/2, +/-[pi]/2), (-[pi]/2, +/-[pi]/2). Group theory shows that this symmetry breaking (SB) may be stable if H' is taken into account. For the two-hole system we derive candidates for the H0-GS with the corresponding good quantum numbers k and total spin S. Here we find no SB or a two-fold SB: again, this result may hold for the complete HA-H. Second order perturbation calculation possibly describes an effective coupling of two holes.}
}

