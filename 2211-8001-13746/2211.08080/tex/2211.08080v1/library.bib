year = {2017}
}
@article{Troni2014,
abstract = {This paper reports on a novel method for estimating the sensor bias{\$}\backslash{\$}nof three-axis magnetometers (or any other field sensor). Our approach{\$}\backslash{\$}nemploys relative angular position measurements to estimate the three-axis{\$}\backslash{\$}nmagnetometer measurement bias, significantly improving magnetometer-based{\$}\backslash{\$}nattitude estimation. Relative angular position measurements can be{\$}\backslash{\$}ncalculated from a variety of sources, including multiview image registration{\$}\backslash{\$}nor laser-based scan matching. We report two methods implementing{\$}\backslash{\$}nthis approach based on batch linear least squares and a real-time{\$}\backslash{\$}ndiscrete Kalman filter. Compared with previously reported methods{\$}\backslash{\$}nour approach is time independent and less restrictive with data sampling.{\$}\backslash{\$}nIn addition, our two methods (i) are empirically shown to impose{\$}\backslash{\$}nless restrictive conditions for the movements of the instrument required{\$}\backslash{\$}nfor calibration, (ii) do not require knowledge of the direction of{\$}\backslash{\$}nthe field (e.g., the local magnetic field) or the attitude of the{\$}\backslash{\$}ninstrument, and (iii) also ensure convergence for the estimated parameters.{\$}\backslash{\$}nThe proposed methods are evaluated and compared with previously reported{\$}\backslash{\$}nmethods in both numerical simulation and in comparative experimental{\$}\backslash{\$}nevaluation using cameras and magnetometer sensors under different{\$}\backslash{\$}nconditions.},
author = {Troni, Giancarlo and Eustice, Ryan M},
doi = {10.1109/IROS.2014.6942597},
isbn = {9781479969340},
issn = {21530866},
journal = {IEEE International Conference on Intelligent Robots and Systems},
pages = {444--450},
title = {{Magnetometer bias calibration based on relative angular position: Theory and experimental comparative evaluation}},
year = {2014}
}
