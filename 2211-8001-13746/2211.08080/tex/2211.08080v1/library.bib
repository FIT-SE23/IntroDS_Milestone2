doi = {10.1016/j.jclepro.2016.06.172},
file = {:home/carlos/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/AlFaris, Juaidi, Manzano-Agugliaro - 2016 - Improvement of efficiency through an energy management program as a sustainable practice in.pdf:pdf},
issn = {09596526},
journal = {Journal of Cleaner Production},
pages = {794--805},
title = {{Improvement of efficiency through an energy management program as a sustainable practice in schools}},
volume = {135},
year = {2016}
}
