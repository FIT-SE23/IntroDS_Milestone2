	keywords = {Algorithm design and analysis, bandwidth, bit rate, cell splitting, channel allocation, channel state information, Computer networks, coordinated technique, {CSI}, Decision making, innovative technique, multicellular {OFDMA} networks, {OFDM}, {OFDM} modulation, power control, power control, Power control, radio access networks, random technique, real broadband wireless access, sequential technique, subcarrier allocation algorithms, telecommunication computing, Transmitters},
	title = {Subcarrier allocation algorithms for multicellular {OFDMA} networks without Channel State Information},
