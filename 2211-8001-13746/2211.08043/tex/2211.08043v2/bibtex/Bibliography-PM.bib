@article{BS00,
	abstract = {It is known that various deterministic and stochastic processes such as asymptotically autonomous differential equations or stochastic approximation processes can be analyzed by relating them to an appropriately chosen semiflow. Here, we introduce the notion of a stochastic process X being a weak asymptotic pseudotrajectory for a semiflow $\Phi$ and are interested in the limiting behavior of the empirical measures of X. The main results are as follows: (1) the weak* limit points of the empirical measures for X axe almost surely $\Phi$-invariant measures; (2) given any semiflow $\Phi$, there exists a weak asymptotic pseudotrajectory X of $\Phi$ such that the set of weak* limit points of its empirical measures almost surely equal the set of all ergodic measures for $\Phi$; and (3) if X is an asymptotic pseudotrajectory for a semiflow $\Phi$, then conditions on $\Phi$ that ensure convergence of the empirical measures are derived.},
	author = {Michel Bena{\"\i}m and Sebastian J. Schreiber},
	date-added = {2016-10-13 23:46:31 +0000},
	date-modified = {2016-10-13 23:46:31 +0000},
	journal = {Journal of Dynamics and Differential Equations},
	number = {3},
	pages = {579-598},
	title = {Ergodic properties of weak asymptotic pseudotrajectories for semiflows},
	volume = {12},
	year = {2000},
	bdsk-url-1 = {http://dx.doi.org/10.1023/A:1026463628355}}

