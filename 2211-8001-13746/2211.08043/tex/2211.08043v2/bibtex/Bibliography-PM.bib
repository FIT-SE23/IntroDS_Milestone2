@inproceedings{HGGB03,
	abstract = {This paper presents a new distributed power control algorithm for ad-hoc wireless networks in random channel environments. Previous work in this area has focused on distributed power control for ad-hoc networks with fixed channels. We show that the algorithms resulting from such formulations do not accurately capture the dynamics of a time-varying channel. The performance of the network, in terms of power consumption and generated interference, can be severely degraded when a power control algorithm designed for a deterministic channel is applied to a random channel. In particular, some well-known strong optimality results for such algorithms no longer hold. In order to address these problems we propose a new criterion for power optimality in ad-hoc wireless networks. We then show that the optimal power allocation for this new criterion can be found through an appropriate stochastic approximation algorithm. Ultimately, the iterations of the stochastic approximation algorithm can be decoupled to form an optimal fully distributed on-line power control algorithm for ad-hoc wireless networks with time-varying channels. 1},
	author = {Tim Holliday and Nicholas Bambos and Peter W. Glynn and Andrea Goldsmith},
	booktitle = {Allerton '03: Proceedings of the 2003 Allerton Conference on Communications, Control, and Computing},
	date-added = {2014-09-28 21:00:35 +0000},
	date-modified = {2021-05-07 02:20:02 +0300},
	file = {Citeseer - Full Text PDF:/home/at-ral/steiakog/.zotero/zotero/u4gcyosa.default/zotero/storage/GJD8JQ7P/Holliday et al. - 2003 - Distributed power control for time varying wireles.pdf:application/pdf},
	shorttitle = {Distributed power control for time varying wireless networks},
	title = {Distributed power control for time varying wireless networks: Optimality and convergence},
	year = {2003}}

