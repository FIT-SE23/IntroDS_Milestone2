@article{homrighausenAnomalousDynamicalPhase2017,
  title = {Anomalous Dynamical Phase in Quantum Spin Chains with Long-Range Interactions},
  author = {Homrighausen, Ingo and Abeling, Nils O. and {Zauner-Stauber}, Valentin and Halimeh, Jad C.},
