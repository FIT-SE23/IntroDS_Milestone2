@Misc{width,
  Note                     = {The width of the atomic beam should roughly match
the width of the part of the laser beam used for its manipulation.
This is necessary because the intensity profile of a laser beam
can only be varied on the wavelength scale. The flexibility of the
approach presented here depends on the flexibilty of the laser
beam profile. At their waist such laser beams are a few
wavelengths across, at least a few micrometers. If the
intersection is sought at another part of the laser beam this may
increase correspondingly to hundreds of micrometres or more.
Atomic beams with such widths are easily created~\cite{Pfau93}.}
}

@Misc{WignerDist_is_special,
  Note                     = {{Out of various quantum phase space distributions~\cite{Hillery_PR84}, only
 Wigner's simultaneously yields the correct projections in
 Schr\"odinger's position $\varrho(x,x,t)=\int dp \; W(x,p,t)$
 and momentum representation $\tilde \varrho(p,p,t)=\int
 dx \; W(x,p,t)$, while maintaining its form~(\ref{eq:WMT}) when evolved
 in time and giving the overlap between states in the simple
 form~$|\langle \psi_1(x,t) | \psi_2(x,t) \rangle |^2 = 2 \pi \hbar \iint
 dx dp \; W_1(x,p,t) \; W_2(x,p,t)$.}}
}

