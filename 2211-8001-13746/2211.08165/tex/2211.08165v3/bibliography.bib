  url           = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-0034861128&partnerID=40&md5=f8c9edb89c0e1042d0e73a4080c62ced},
}

@Article{Ozana2020,
  author    = {Ozana, Stepan and Docekal, Tomas and Kawala-Sterniuk, Aleksandra and Mozaryn, Jakub and Schlegel, Milos and Raj, Akshaya},
