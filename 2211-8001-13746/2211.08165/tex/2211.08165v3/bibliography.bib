}

@Article{Bobenko2003,
  author   = {Bobenko, Alexander. I. and Matthes, Daniel and Suris, Yuri B.},
  title    = {{Discrete and smooth orthogonal systems: $C^\infty$-approximation}},
  doi      = {10.1155/S1073792803130991},
  eprint   = {http://oup.prod.sis.lan/imrn/article-pdf/2003/45/2415/2046901/2003-45-2415.pdf},
  issn     = {1073-7928},
  number   = {45},
  pages    = {2415-2459},
  volume   = {2003},
  abstract = {{Discrete conjugate systems are quadrilateral nets with all planar faces. Discrete orthogonal systems are defined by the additional property of all faces being concircular. Their geometric properties allow one to consider them as proper discretization of conjugate, respectively, orthogonal coordinate systems of classical differential geometry. We develop techniques that allow us to extend this known qualitative analogy to rigorous convergence results. In particular, we prove the C∞-convergence of discrete conjugate/orthogonal coordinate systems to smooth ones. We also show how to construct the approximating discrete nets. Coordinate systems and their transformations are treated on an equal footing, and the approximation results hold for transformations as well.}},
  file     = {:Bobenko2003.pdf:PDF},
  journal  = {International Mathematics Research Notices},
  month    = jan,
  year     = {2003},
}

