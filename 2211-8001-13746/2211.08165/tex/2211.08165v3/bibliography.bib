@InProceedings{Stadlmayr2016,
  author    = {Stadlmayr, Daniel and Witteveen, Wolfgang},
  booktitle = {Nonlinear Dynamics, Volume 1},
  title     = {Model Reduction for Nonlinear Multibody Systems Based on Proper Orthogonal-and Smooth Orthogonal Decomposition},
  year      = {2016},
  address   = {Cham},
