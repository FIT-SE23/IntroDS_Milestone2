@Article{Feng2012,
  author          = {Feng, Y. and Yao-Nan, W. and Yi-Min, Y.},
  journal         = {International Journal of Computers, Communications and Control},
  title           = {Inverse kinematics solution for robot manipulator based on neural network under joint subspace},
  year            = {2012},
  note            = {cited By 21},
  number          = {3},
  pages           = {459-472},
  volume          = {7},
  abstract        = {Neural networks with their inherent learning ability have been widely applied to solve the robot manipulator inverse kinematics problems. However, there are still two open problems: (1) without knowing inverse kinematic expressions, these solutions have the difficulty of how to collect training sets, and (2) the gradient-based learning algorithms can cause a very slow training process, especially for a complex configuration, or a large set of training data. Unlike these traditional implementations, the proposed metho trains neural network in joint subspace which can be easily calculated with electromagnetism-like method. The kinematics equation and its inverse are one-to-one mapping within the subspace. Thus the constrained training sets can be easily collected by forward kinematics relations. For issue 2, this paper uses a novel learning algorithm called extreme learning machine (ELM) which randomly choose the input weights and analytically determines the output weights of the single hidden layer feedforward neural networks (SLFNs). In theory, this algorithm tends to provide the best generalization performance at extremely fast learning speed. The results show that the proposed approach has not only greatly reduced the computation time but also improved the precision. © 2006-2012 by CCC Publications.},
  affiliation     = {The College of Electrical and Information Engineering, Hunan University, Changsha, Hunan Province 410082, China},
  author_keywords = {Extreme learning machine; Inverse kinematics; Neural network},
  document_type   = {Article},
  file            = {:Feng2012.pdf:PDF},
  source          = {Scopus},
  url             = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84862691584&partnerID=40&md5=28d6e96f828f7fe71fe4d5e8a58e0894},
}

@Article{Ficuciello2015,
  author   = {Fanny {Ficuciello} and Luigi {Villani} and Bruno {Siciliano}},
  journal  = {IEEE Transactions on Robotics},
  title    = {Variable Impedance Control of Redundant Manipulators for Intuitive Human–Robot Physical Interaction},
  year     = {2015},
  issn     = {1941-0468},
  month    = aug,
  number   = {4},
  pages    = {850-863},
  volume   = {31},
  abstract = {This paper presents an experimental study on human-robot comanipulation in the presence of kinematic redundancy. The objective of the work is to enhance the performance during human-robot physical interaction by combining Cartesian impedance modulation and redundancy resolution. Cartesian impedance control is employed to achieve a compliant behavior of the robot's end effector in response to forces exerted by the human operator. Different impedance modulation strategies, which take into account the human's behavior during the interaction, are selected with the support of a simulation study and then experimentally tested on a 7-degree-of-freedom KUKA LWR4. A comparative study to establish the most effective redundancy resolution strategy has been made by evaluating different solutions compatible with the considered task. The experiments have shown that the redundancy, when used to ensure a decoupled apparent inertia at the end effector, allows enlarging the stability region in the impedance parameters space and improving the performance. On the other hand, the variable impedance with a suitable modulation strategy for parameters' tuning outperforms the constant impedance, in the sense that it enhances the comfort perceived by humans during manual guidance and allows reaching a favorable compromise between accuracy and execution time.},
  doi      = {10.1109/TRO.2015.2430053},
  file     = {:Ficuciello2015.pdf:PDF},
  keywords = {end effectors;human-robot interaction;manipulator kinematics;redundant manipulators;stability;variable impedance control;redundant manipulators;intuitive human-robot physical interaction;human-robot comanipulation;kinematic redundancy;Cartesian impedance modulation strategy;robot end effector compliant behavior;7-degree-of-freedom KUKA LWR4;effective redundancy resolution strategy;decoupled apparent inertia;stability region;parameter tuning;Impedance;End effectors;Redundancy;Stability analysis;Joints;Dynamics;Force control;physical human–robot interaction;redundant robots;Force control;physical human–robot interaction;redundant robots},
}

