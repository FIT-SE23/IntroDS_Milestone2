  booktitle = {Ergodic Theory and Dynamical Systems in their Interactions with Arithmetics and Combinatorics},
  publisher = {Springer International Publishing},
  title     = {A {Brief} {Guide} to {Reversing} and {Extended} {Symmetries} of {Dynamical} {Systems}},
  year      = {2018},
