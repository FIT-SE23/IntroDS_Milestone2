@Article{Lorensen1987,
  author     = {Lorensen, William E. and Cline, Harvey E.},
  journal    = {SIGGRAPH Comput. Graph.},
  title      = {Marching Cubes: A High Resolution 3D Surface Construction Algorithm},
  year       = {1987},
  issn       = {0097-8930},
