@InProceedings{Lammert2010,
  author          = {Lammert, A.C. and Goldstein, L. and Iskarous, K.},
  booktitle       = {Proceedings of the 11th Annual Conference of the International Speech Communication Association, INTERSPEECH 2010},
  title           = {Locally-weighted regression for estimating the forward kinematics of a geometric vocal tract model},
  year            = {2010},
  note            = {cited By 4},
  pages           = {1604-1607},
  abstract        = {Task-space control is well studied in modeling speech production [1, 2, 3, 4]. Implementing control of this kind requires an accurate kinematic forward model. Despite debate about how to define the tasks for speech (i.e., acoustical vs. articulatory), a faithful forward model will be complex and infeasible to express analytically. Thus, it is necessary to learn the forward model from data. Artificial Neural Networks (ANNs) have previously been suggested for this [3, 4, 6]. We argue for the use of locally-linear methods, such as Locally-Weighted Regression (LWR). While ANNs are capable of learning complex forward maps, LWR is more appropriate. Common formulations of control assume locally-linearity, whereas ANNs fit a nonlinear model to the entire map. Likewise, training LWR is simple compared to the complex optimization for ANNs. We provide an empirical comparison of these methods for learning a vocal tract forward model, discussing theoretical and practical aspects of each. © 2010 ISCA.},
  affiliation     = {Department of Computer Science, University of Southern California, CA, United States; Department of Linguistics, University of Southern California, CA, United States; Haskins Laboratories, New Haven, CT, United States},
  author_keywords = {Forward kinematics; Forward model estimation; Speech production; Statistical machine learning; Task dynamics},
  document_type   = {Conference Paper},
  journal         = {Proceedings of the 11th Annual Conference of the International Speech Communication Association, INTERSPEECH 2010},
  source          = {Scopus},
  url             = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-79959829039&partnerID=40&md5=990dcd182f46dd3817acff65751959f7},
}

