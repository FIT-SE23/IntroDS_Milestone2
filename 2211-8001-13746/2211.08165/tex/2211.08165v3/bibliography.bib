@TechReport{Sachtler2016,
  author   = {Arne Sachtler},
  title    = {Manuelle Registrierung von Objekten in Roboterarbeitszellen unter Verwendung von Wissensdatenbanken, Automatisierte Selbstkalibrierung von Robotern in der Arbeitszelle},
  year     = {2016},
  month    = sep,
  type     = {T2000},
  abstract = {Conventional robotics programming is affected by specification and computation of poses in three-
dimensional space. In all robotic fields engineers must know about rigid body kinematics and spatial
transformations. A comparatively new approach is the specification of motions and tasks by means of
object’s names instead of poses. At runtime names can be resolved to concrete transformations by
database resolution. Another important benefit in using object’s names is the usability in artificial
intelligence concerning robotics. As soon as a robotics system knows a name of any object and not
exclusively poses, there is much more knowledge about the world’s state that can be used in AI agents
or similar systems for high level reasoning on a symbolic level. Tasks and motions can be described on
an abstract level and it is easier to perform automated creation of tasks and sequences.
In such a robotic system there needs to be a central knowledge database to resolve names to spatial
pose information. That database has to be filled with these. The first part of this report will develop
a method to register objects in an object’s database. Spatial transformations are determined using
the robots forward kinematics itself. Objects are grasped with the robot’s gripper and the robot is
moved to the object’s pose using gravitation compensation mode. In that process many samples are
generated and a single transformation is computed by developed methods.
The second part develops a method to perform automated robot-world calibration for multiple robots.
A calibration frame is placed in the world and all robots grasp it autonomously on multiple points,
which are related by a geometric plane constraint. Eventually the robot’s pose in the world is estimated
based on observations made during autonomous grasping.},
  file     = {:Sachtler2016.pdf:PDF},
  school   = {German Aerospace Center},
}

@TechReport{Sachtler2017,
  author   = {Arne Sachtler},
  title    = {Visual-Tactile Registration of Objects in Robotic Assembly Systems},
  year     = {2017},
  month    = sep,
  abstract = {The information about the accurate position and orientation of an object relative to a
robot plays a fundamental role in robotic manipulation. Registration refers to the process
of determining that so-called object pose. Generally, registration is performed manually
and must be executed with high precision. For flexible systems, an automated approach,
where the robotic system determines poses of objects automatically is desirable. This
work proposes a visual-tactile feature-based approach for object registration.
Features describe distinctive properties of objects that can be observed by sensors.
By using a Bayesian inference approach, the system explicitly models hypotheses and
uncertainties of feature poses and infers the hidden world state from measurements. In
this work, recursive state estimation is used in order to fuse visual and tactile data.
In particular, a sequential Monte Carlo algorithm, namely a particle filter, is applied.
Additionally, periodically the belief on the current world state is analysed in order to
detect and characterise uncertainties. Based on that, uncertainty lowering strategies are
selected, parametrised and automatically executed by the robot.
Eventually, the approach is experimentally validated within a robotic work cell. There-
fore, hole-shaped features of objects are used. Comparison of solely visual and fused
visual-tactile results shows that the addition of tactile measurements and entropy redu-
cing strategies increases information and lowers the uncertainty of the believed world
state significantly. At the same time better feature poses for object registration are
generated.},
  file     = {:Sachtler2017.pdf:PDF},
  keywords = {Object registration, state estimation},
  school   = {German Aerospace Center, Institute of Robotics and Mechatronics},
  url      = {https://elib.dlr.de/114596/},
}

@TechReport{Sachtler2017a,
  author   = {Arne Sachtler},
  title    = {Camera-supported Automatic Calibration of Robotic Workcells},
  year     = {2017},
  month    = apr,
  type     = {T3000},
  abstract = {In order to interact with objects or other robots, a robot system essentially requires precise
information on the poses of objects. In this report a completely automatic calibration
method is proposed. By combining optic and haptic sensory measurements, the poses of
the robots in a world frame are estimated. Basically a camera system mounted in the
work cell, detects graspable features visually and by transforming the poses of detected
features into the base frame of the robot, the features can be investigated haptically.
Therefore, advantage of stiffness control and tactile capabilities of the robot, is made to
improve the estimates. Haptic alignment to the object is done using impedance controllers.
Thus, optical inaccuracies are compensated haptically. Based on measurements, the robot
took during haptic investigation, the robot pose can be estimated, using knowledge about
the location of the features in the work cell. As no a priori knowledge on the kinematic
configuration is available, also an automatic robot to camera calibration is developed and
implemented. As object and optic feature recognition is not the main task of this report,
it is substituted by a visual marker system, that replaces tough recognition algorithms.
Additionally a graphical user interface to the implemented methods is developed. It
provides a set of calibration assistants, whose objective is to guide the operator through
the calibration process intuitively.},
  file     = {:Sachtler2017a.pdf:PDF},
  school   = {German Aerospace Center},
}

@TechReport{Sachtler2017b,
  author   = {Arne Sachtler},
  title    = {Online Reconstruction of Three-Dimensional Ball Trajectories from Multiple Planar Images under Perspective Projection},
  year     = {2017},
  address  = {Mannheim},
  month    = jun,
  type     = {Studienarbeit},
  abstract = {Teams from all over the world play soccer games with robots, that are completely con-
trolled autonomously by a computer system, in the RoboCup small size league (SSL).
A shared computer vision system provides locations of the robots and the ball for both
teams. The recognition of robots and the ball is based on the detection of colour-patterns
in the images of multiple cameras placed above the field. Generally, pose information is
determined based on the constraint that all objects share the same plane in the world.
Indeed that is true for robots as they usually don’t fly. The planar constraint is used for
the ball as well, which is resulting in wrong locations and so-called virtual balls. These
location information and corresponding timestamps provided by the shared computer
vision system are used by the TIGERs’ filter algorithms to predict the future state of
the ball. In this work an algorithm is developed that reconstructs the ball trajectories in
three-dimensional space based on the perspective projections of their trajectory parabo-
las. Reconstructed trajectories are used in the filters to predict the ball’s future state
properly.},
  file     = {:Sachtler2017b.pdf:PDF},
  school   = {Baden-Württemberg Cooperative State University (DHBW)},
}

