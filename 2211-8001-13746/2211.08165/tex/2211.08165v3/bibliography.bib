@InProceedings{Baker1987,
  author    = {D. {Baker} and C. {Wampler}},
  booktitle = {Proceedings. 1987 IEEE International Conference on Robotics and Automation},
  title     = {Some facts concerning the inverse kinematics of redundant manipulators},
  year      = {1987},
