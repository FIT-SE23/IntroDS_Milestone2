@TechReport{Nottensteiner2012,
  author   = {Korbinian Nottensteiner},
  title    = {Sch{\"a}tzung des Kontaktzustands bei robotergest{\"u}tzten Montageaufgaben},
  year     = {2012},
  month    = jun,
  abstract = {Eine Herausforderung in der robotergest{\"u}tzten Montage mit dem KUKA Leichtbauroboter
liegt in der automatischen Beurteilung, ob ein Montageprozess erfolgreich
abgelaufen ist. Im hier entwickelten Sch{\"a}tzverfahren wird {\"u}berpr{\"u}ft, ob die gemessenen
Drehmomente und Geschwindigkeiten mit dem angenommenen Kontaktzustand
konsistent sind. Alle geometrischen Unsicherheiten in der kinematischen Kette zwischen
Roboter und Umgebung werden in einem Starrk{\"o}rpermodell zu einer einzigen
relativen Unsicherheit zusammengefasst, die mit einer sequentiellen Monte-Carlo
Methode in der Form des Bootstrap-Filters gesch{\"a}tzt wird. Die kinematischen Einschr{\"a}nkungen
im Kontakt werden durch virtuelle Kontaktmanipulatoren repr{\"a}sentiert.
Mehrfachkontakte werden mit Methoden der Grassmann-Cayley Algebra beschrieben.
Eine Validierung erfolgt mit Hilfe von analytischen Kontaktmodellen f{\"u}r das klassische
Peg-In-Hole Problem. Mehrere Experimente wurden hierf{\"u}r mit dem Leichtbauroboter
durchgef{\"u}hrt und offline ausgewertet.},
