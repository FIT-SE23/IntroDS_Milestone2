@InProceedings{Parikh2002,
  author        = {Parikh, P.J. and Lin, C.H. and Lam, S.S.Y.},
  booktitle     = {Intelligent Engineering Systems Through Artificial Neural Networks},
  title         = {Approximating the forward kinematics solution for a stewart platform using two concepts of neural networks},
  year          = {2002},
  note          = {cited By 1},
  pages         = {847-852},
  volume        = {12},
  abstract      = {The paper discusses about a kinematic problem related to a stewart platform used for simulating aircraft kinematics. Two issues need to be resolved for this mechanism, the forward kinematics and the inverse kinematics. The problem is to obtain the forward kinematics solution, which currently is achieved using a numerical technique, Newton-Raphson method. This technique, though closely approximates the solution to the problem, is computationally very intensive. Neural networks, with their inherent ability to approximate any unknown function and fast execution characteristics, have been employed as an attempt to obtain a real-time solution to this problem. Two concepts in neural networks, All Class One Network (ACON) and One Class One Network (OCON), have been investigated using the multi-layered perceptron with backpropagation learning algorithm. The experiments indicated that ACON is significantly better than OCON considering the network complexity, computational resource utilization, and better output parameter estimations.},
  affiliation   = {Dept. of Syst. Sci./Indust. Eng., Binghamton University, Binghamton, NY 13902, United States; Dept. of Elec./Computer Engineering, Binghamton University, Binghamton, NY 13902, United States},
