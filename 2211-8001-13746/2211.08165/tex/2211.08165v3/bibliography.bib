@Book{Dekking2005,
  author     = {Dekking, F. M. and Kraaikamp, C. and Lopuhaä, H. P. and Meester, L. E.},
  publisher  = {Springer Science \& Business Media},
  title      = {A {Modern} {Introduction} to {Probability} and {Statistics}: {Understanding} {Why} and {How}},
